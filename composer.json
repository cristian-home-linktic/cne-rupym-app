{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^11.40.0",
        "laravel/horizon": "^5.30.2",
        "laravel/reverb": "^1.4.5",
        "laravel/sanctum": "^4.0.7",
        "laravel/scout": "^10.12.1",
        "laravel/telescope": "^5.3.0",
        "laravel/tinker": "^2.10",
        "league/flysystem-aws-s3-v3": "^3.29",
        "meilisearch/meilisearch-php": "^1.12",
        "silber/bouncer": "^1.0",
        "spatie/laravel-activitylog": "^4.9.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "barryvdh/laravel-ide-helper": "^3.5",
        "brianium/paratest": "^7.7",
        "captainhook/captainhook": "^5.24.3",
        "fakerphp/faker": "^1.24.1",
        "jasonmccreary/laravel-test-assertions": "^2.4.1",
        "larastan/larastan": "^3.0.2",
        "laravel-shift/blueprint": "^2.11",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.1",
        "laravel/pint": "^1.20.0",
        "laravel/sail": "^1.40.0",
        "mockery/mockery": "^1.6.12",
        "nikic/php-parser": "^5.4.0",
        "nunomaduro/collision": "^8.6.1",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -N"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
